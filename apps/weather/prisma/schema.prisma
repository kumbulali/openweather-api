generator client {
  provider      = "prisma-client-js"
  output        = "../node_modules/.prisma/client"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Weatherquery {
  id        Int      @id @default(autoincrement())
  path      String
  method    String
  timestamp DateTime
  duration  Int
  userId    Int
  status    Int
}
